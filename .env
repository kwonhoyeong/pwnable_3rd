# Database - Docker Compose용 개별 변수
NT_POSTGRES_USER=ntuser
NT_POSTGRES_PASSWORD=ntpassword
NT_POSTGRES_DB=threatdb

# Database - 로컬 개발용 DSN (Docker에서는 postgres 호스트, 로컬에서는 localhost)
NT_POSTGRES_DSN=postgresql+asyncpg://ntuser:ntpassword@localhost:5432/threatdb

# Redis
NT_REDIS_URL=redis://localhost:6379/0

# AI API Keys (팀원별로 설정 필요)
NT_PERPLEXITY_API_KEY=your-perplexity-api-key
NT_CLAUDE_API_KEY=your-claude-api-key
NT_GPT5_API_KEY=your-gpt5-api-key

# Logging
NT_LOG_LEVEL=INFO
